<resources>

    <style name="cc_id_type_dlg_cl">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center_horizontal</item>
        <item name="layout_constraintTop_toTopOf">parent</item>
        <item name="layout_constraintBottom_toBottomOf">parent</item>
        <item name="android:paddingBottom">27.71dp</item>
        <item name="layout_constraintStart_toStartOf">parent</item>
        <item name="layout_constraintEnd_toEndOf">parent</item>
        <item name="android:background">@drawable/dlg_bckgrd</item>
    </style>

    <style name="cc_id_type_dlg_top_img">
        <item name="android:layout_width">88.89dp</item>
        <item name="android:layout_height">64dp</item>
        <item name="layout_constraintTop_toTopOf">parent</item>
        <item name="android:layout_marginTop">8dp</item>
        <item name="layout_constraintStart_toStartOf">parent</item>
        <item name="layout_constraintEnd_toEndOf">parent</item>
        <item name="android:src">@drawable/ic_passport</item>
    </style>

    <style name="cc_id_type_dlg_heading" parent="TextAppearance.AppCompat">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="layout_constraintStart_toStartOf">parent</item>
        <item name="android:layout_marginLeft">52dp</item>
        <item name="layout_constraintEnd_toEndOf">parent</item>
        <item name="android:layout_marginRight">63dp</item>
        <item name="android:text">Country And ID Type</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">#191D31</item>
        <item name="android:textFontWeight">600</item>
        <item name="android:lineSpacingExtra">2sp</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="cc_id_type_dlg_input_label" parent="TextAppearance.AppCompat">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">24dp</item>
        <item name="android:layout_marginTop">24dp</item>
        <item name="layout_constraintStart_toStartOf">parent</item>
        <item name="android:layout_marginLeft">20dp</item>
        <item name="android:layout_marginRight">20dp</item>
        <item name="android:textColor">#252C32</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textFontWeight">400</item>
        <item name="android:lineHeight">24sp</item>
        <item name="android:letterSpacing">-0.01</item>
        <item name="android:fontFamily">@font/inter</item>
        <item name="android:gravity">left|center_vertical</item>
    </style>

    <style name="cc_id_type_dlg_input_field" parent="TextAppearance.AppCompat">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:layout_marginTop">6dp</item>
        <item name="layout_constraintStart_toStartOf">parent</item>
        <item name="android:layout_marginLeft">20dp</item>
        <item name="layout_constraintEnd_toEndOf">parent</item>
        <item name="android:layout_marginRight">20dp</item>
        <item name="android:paddingTop">8dp</item>
        <item name="android:paddingBottom">8dp</item>
        <item name="android:paddingLeft">8dp</item>
        <item name="android:paddingRight">16dp</item>
        <item name="android:drawablePadding">8dp</item>
        <item name="android:drawableLeft">@null</item>
        <item name="android:drawableRight">@drawable/ic_down_arrow</item>
        <item name="android:background">@drawable/id_info_input_bckgrd</item>
        <item name="android:fontFamily">@font/inter</item>
        <item name="android:fontWeight">400</item>
        <item name="android:lineHeight">24dp</item>
        <item name="android:letterSpacing">-0.01</item>
        <item name="android:lineSpacingExtra">7sp</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#9AA6AC</item>
        <item name="android:gravity">left|center_vertical</item>
    </style>

    <style name="cc_id_type_dlg_country_picker">
        <item name="android:layout_width">0dp</item>
        <item name="layout_constraintWidth_percent">0.6</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">8dp</item>
        <item name="layout_constraintStart_toStartOf">parent</item>
        <item name="android:layout_marginLeft">16dp</item>
        <item name="ccpDialog_textColor">@color/darkerGray</item>
        <item name="ccpDialog_backgroundColor">@color/white_color</item>
        <item name="ccp_textSize">16dp</item>
        <item name="ccp_arrowSize">24dp</item>
        <item name="ccp_showFullName">true</item>
        <item name="ccp_showNameCode">false</item>
        <item name="ccp_showPhoneCode">false</item>
        <item name="ccp_defaultPhoneCode">234</item>
    </style>

    <style name="cc_id_type_dlg_id_type_spinner">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">45dp</item>
        <item name="android:layout_marginTop">8dp</item>
        <item name="android:layout_marginLeft">6dp</item>
        <item name="android:background">@color/red_400</item>
        <item name="android:hint">@string/sp_enroll_result_id_type_hint</item>
        <item name="android:ems">10</item>
        <item name="android:textColor">@color/darkerGray</item>
        <item name="android:textColorHint">@color/darkerGray</item>
        <item name="android:popupBackground">@color/white_color</item>
        <item name="android:inputType">textPersonName</item>
    </style>

    <style name="cc_id_type_dlg_proceed_btn" parent="TextAppearance.AppCompat">
        <item name="android:layout_width">299.71dp</item>
        <item name="android:layout_height">49.65dp</item>
        <item name="android:background">@drawable/blue_btn_bckgrd</item>
        <item name="android:layout_marginTop">31dp</item>
        <item name="layout_constraintStart_toStartOf">parent</item>
        <item name="layout_constraintEnd_toEndOf">parent</item>
        <item name="android:text">@string/id_card_back_dlg_proceed_btn</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textFontWeight">600</item>
        <item name="android:gravity">center</item>
        <item name="android:visibility">gone</item>
    </style>

</resources>